{
  "name": "react-nyt-api",
  "version": "0.1.0",
  "description": "Search for and annotate articles of interest",
  "author": "Ben Garcia <ben@seebenprogram.com>",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-client\"",
    "dev-server": "concurrently \"mongod\" \"nodemon ./bin/www\"",
    "dev-client": "cd client && yarn start",
    "dev-server-prod": "concurrently \"mongod\" \"node ./bin/www\""
  },
  "dependencies": {
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "mongoose": "^5.1.4",
    "morgan": "~1.9.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "nodemon": "^1.17.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BenRGarcia/React-NYT-Api"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BenRGarcia/React-NYT-Api/issues"
  }
}
